{
  "name": "spacetime-sdk",
  "version": "0.8.0",
  "description": "The Spacetime standard kit provide developers with turnkey virtual environments. This package will provide multiple metaverse features like multiplayer, custom avatars, vehicle and blockchain connection.",
  "main": "./build/bundle.min.js",
  "engines": {
    "npm": "6.x",
    "node": "14.x"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm cardano && npm run build && npm run dependencies && npm run server",
    "cardano": "cd src/cardano && npm install & cd ../..",
    "dependencies": "cd server && npm install & cd ..",
    "server": "node server/server.js",
    "build": "webpack --mode production",
    
    "local": "webpack serve --config ./webpack.config.js --mode development"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Spacetime-Meta/spacetime-standard-kit.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Spacetime-Meta/spacetime-standard-kit/issues"
  },
  "homepage": "https://spacetimemeta.io",
  "devDependencies": {
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "dependencies": {
    "nipplejs": "^0.9.1",
    "peerjs": "^1.4.6",
    "seedrandom": "^3.0.5",
    "socket.io-client": "^4.5.1",
    "three": "^0.142.0"
  }
}
